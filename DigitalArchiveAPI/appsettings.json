{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AzureSearch": {
    "ServiceName": "<your-azure-search-service-name-here>",
    "ApiKey": "<your-azure-search-service-api-key-here>"
  },
  "AzureStorage": {
    "ConnectionString": "<your-azure-storage-service-connection-string-here>"
  },
  "Feedback": {
    "MailRecipients": [ "somebody@your.org", "someone@your.org" ],
    "MailFrom": "no-reply@your.org",
    "MailSubject": "<your mail subject here>",
    "SendGridApiKey": "<your-azure-sendgrid-api-key-here>"
  }
}
